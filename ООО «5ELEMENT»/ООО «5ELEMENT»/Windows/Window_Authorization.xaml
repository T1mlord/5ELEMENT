<Window x:Class="ООО__5ELEMENT_.Windows.Window_Authorization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ООО__5ELEMENT_.Windows"
        mc:Ignorable="d"
        Title="Авторизация" Height="500" Width="600" MinHeight="500" MinWidth="600">
    <Grid Background="#FF9B9B9B">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="1" x:Name="Frame_Authorization" Margin="0,0,-2,29" Grid.RowSpan="2"/>
        <TextBlock Grid.Row="2" Text="Все права принадлежат ООО «5ELEMENT»" FontFamily="Comic Sans MS" VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</Window>
