<Page x:Class="ООО__5ELEMENT_.Pages.Employees.Stockman.Page_PeriodReport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ООО__5ELEMENT_.Pages.Employees.Stockman"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_PeriodReport">

    <Grid>
        <StackPanel>
            <DataGrid x:Name="DataGrid_PeriodReport" Height="360"/>
            <Grid>
                <DatePicker x:Name="DatePicker_Begin" Width="170" SelectedDateChanged="DatePicker_End_SelectedDateChanged" HorizontalAlignment="Left"/>
                <StackPanel Width="130"/>
                <DatePicker x:Name="DatePicker_End" Width="170" SelectedDateChanged="DatePicker_End_SelectedDateChanged" HorizontalAlignment="Center"/>
                <StackPanel Width="130"/>
                <Button x:Name="Button_Print" Height="30" Width="200" Content="Составить отчёт" FontFamily="Comic Sans MS" Click="Button_Print_Click" Foreground="Black" Background="Cyan" HorizontalAlignment="Right"/>
            </Grid>
        </StackPanel>
    </Grid>
</Page>
